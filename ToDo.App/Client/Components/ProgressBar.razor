<div class="progress">
    <div class="progress-bar progress-bar-striped" role="progressbar" style="width: @(Progress.ToString(System.Globalization.CultureInfo.InvariantCulture))%" aria-valuenow="@Progress" aria-valuemin="0" aria-valuemax="100">@Progress %</div>
</div>
@code {
    [Parameter]
    public double Progress { get; set; }
}
